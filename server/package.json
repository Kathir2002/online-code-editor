    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^5.0.0-beta.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth2": "^0.2.0"
  },
  "devDependencies": {
