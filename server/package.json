{
  "name": "sniplet-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean-dist": "if exist dist rmdir /s /q dist && mkdir dist || mkdir dist",
    "build": "npm run clean-dist && tsc && npm run copy-files && npm run copy-node_modules",
    "copy-files": "xcopy src dist\\src /E /H /C /I && xcopy *.json dist /C /I && xcopy .env dist /C /I && xcopy .catalystrc dist /C /I",
    "copy-node_modules": "xcopy node_modules dist\\node_modules /E /H /C /I",
    "start": "node index.js",
    "dev": "nodemon index.ts",
    "deploy": "npm run build && cd dist && catalyst deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
